# 1 "overlays/ebf/imx-fire-485r2-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "overlays/ebf/imx-fire-485r2-overlay.dts"
# 10 "overlays/ebf/imx-fire-485r2-overlay.dts"
/dts-v1/;
/plugin/;
# 1 "include/imx6ul-pinfunc.h" 1
# 13 "overlays/ebf/imx-fire-485r2-overlay.dts" 2
# 1 "include/dt-bindings/gpio/gpio.h" 1
# 14 "overlays/ebf/imx-fire-485r2-overlay.dts" 2



/ {
     fragment@0 {
        target=<&uart3>;
        __overlay__ {
     pinctrl-names = "default";
     pinctrl-0 = <&pinctrl_485>;
     fsl,rs485-gpio-txen = <&gpio1 23 0>;
     linux,rs485-enabled-at-boot-time;
     status = "okay";
        };
     };
    fragment@1 {
        target=<&iomuxc>;
        __overlay__ {
     pinctrl_485: 485grp {
      fsl,pins = <
       0x00a4 0x0330 0x0000 0 0 0x1b0b1
       0x00a8 0x0334 0x0634 0 1 0x1b0b1
       0x00a0 0x032c 0x0000 5 0 0x1b0b1
      >;
     };

        };
    };
};
