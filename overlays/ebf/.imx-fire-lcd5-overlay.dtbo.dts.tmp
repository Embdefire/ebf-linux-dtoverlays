                pinctrl_i2c1: i2c1grp {
                    fsl,pins = <
                        0x00b4 0x0340 0x05a4 2 1 0x4001b8b0
                        0x00b8 0x0344 0x05a8 2 2 0x4001b8b0
                    >;
                };
        pinctrl_pwm1: pwm1grp {
         fsl,pins = <
          0x007c 0x0308 0x0000 0 0 0x000010B1
         >;
        };
                pinctrl_tsc_reset: tscresetgrp {
                    fsl,pins = <

                        0x0114 0x03a0 0x0000 5 0 0x05
                    >;
                };
            };
        };
        fragment@3 {
            target = <&iomuxc_snvs>;
            __overlay__ {
                pinctrl_tsc_irq: tsc_irq {
                    fsl,pins = <
                        0x002C 0x0070 0x0000 0x5 0x0 0x4001b8b0
                    >;
