# 1 "overlays/ebf/imx-fire-485r1-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "overlays/ebf/imx-fire-485r1-overlay.dts"
# 10 "overlays/ebf/imx-fire-485r1-overlay.dts"
/dts-v1/;
/plugin/;
# 1 "include/imx6ul-pinfunc.h" 1
# 13 "overlays/ebf/imx-fire-485r1-overlay.dts" 2
# 1 "include/dt-bindings/gpio/gpio.h" 1
# 14 "overlays/ebf/imx-fire-485r1-overlay.dts" 2



/ {
     fragment@0 {
        target=<&uart2>;
        __overlay__ {
     pinctrl-names = "default";
     pinctrl-0 = <&pinctrl_485>;
     fsl,rs485-gpio-txen = <&gpio1 22 0>;
     linux,rs485-enabled-at-boot-time;
     status = "okay";
        };
     };
    fragment@1 {
        target=<&iomuxc>;
        __overlay__ {
     pinctrl_485: 485grp {
      fsl,pins = <
       0x0094 0x0320 0x0000 0 0 0x1b0b1
       0x0098 0x0324 0x062c 0 1 0x1b0b1
       0x009c 0x0328 0x0000 5 0 0x1b0b1
      >;
     };

        };
    };
};
