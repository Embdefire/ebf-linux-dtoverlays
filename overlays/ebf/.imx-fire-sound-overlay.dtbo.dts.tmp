# 1 "overlays/ebf/imx-fire-sound-overlay.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "overlays/ebf/imx-fire-sound-overlay.dts"
# 9 "overlays/ebf/imx-fire-sound-overlay.dts"
# 1 "include/imx6ul-pinfunc.h" 1
# 10 "overlays/ebf/imx-fire-sound-overlay.dts" 2
# 1 "include/dt-bindings/clock/imx6ul-clock.h" 1
# 11 "overlays/ebf/imx-fire-sound-overlay.dts" 2

/dts-v1/;
/plugin/;

/ {
 fragment@0{
  target-path="/";
  __overlay__{
     sound {
   compatible = "fsl,imx6ul-evk-wm8960",
      "fsl,imx-audio-wm8960";
   model = "wm8960-audio";
   cpu-dai = <&sai2>;
   audio-codec = <&codec>;
   asrc-controller = <&asrc>;
   codec-master;
   gpr = <&gpr 4 0x100000 0x100000>;






   hp-det = <3 1>;


   audio-routing =
    "Headphone Jack", "HP_L",
    "Headphone Jack", "HP_R",
    "Ext Spk", "SPK_LP",
    "Ext Spk", "SPK_LN",
    "Ext Spk", "SPK_RP",
    "Ext Spk", "SPK_RN",
    "LINPUT2", "Mic Jack",
    "LINPUT3", "Mic Jack",
    "RINPUT1", "Main MIC",
    "RINPUT2", "Main MIC",
    "Mic Jack", "MICB",
    "Main MIC", "MICB",
    "CPU-Playback", "ASRC-Playback",
    "Playback", "CPU-Playback",
    "ASRC-Capture", "CPU-Capture",
    "CPU-Capture", "Capture";
    status = "okay";
   };
  };
 };


 fragment@1{
  target=<&iomuxc>;
  __overlay__ {
    pinctrl_i2c2: i2c2grp {
    fsl,pins = <
     0x00bc 0x0348 0x05ac 2 2 0x4001b8b0
     0x00c0 0x034c 0x05b0 2 2 0x4001b8b0
    >;
   };
   pinctrl_sai2: sai2grp {
    fsl,pins = <
     0x0050 0x02dc 0x05f8 2 0 0x17088
     0x004c 0x02d8 0x05fc 2 0 0x17088
     0x0058 0x02e4 0x0000 2 0 0x11088
     0x0054 0x02e0 0x05f4 2 0 0x11088
     0x0048 0x02d4 0x05f0 2 0 0x17088
    >;
   };
   pinctrl_spdif: spdifgrp {
     fsl,pins = <
     0x0044 0x02d0 0x0000 2 0 0x1b0b0
     0x0080 0x030c 0x0618 2 0 0x1b0b0
    >;
   };
  };
 };

 fragment@2{
  target=<&i2c2>;
  __overlay__{
   clock_frequency = <100000>;
   pinctrl-names = "default";
   pinctrl-0 = <&pinctrl_i2c2>;
   status = "okay";
     #address-cells = <1>;
     #size-cells = <0>;

   codec: wm8960@1a {
    compatible = "wlf,wm8960";
    reg = <0x1a>;
    clocks = <&clks 180>;
    clock-names = "mclk";
    wlf,shared-lrclk;
   };
  };
 };

 fragment@3 {
  target=<&sai2>;
  __overlay__{
   pinctrl-names = "default";
   pinctrl-0 = <&pinctrl_sai2>;


   assigned-clocks = <&clks 71>,
   <&clks 180>;
   assigned-clock-parents = <&clks 50>;
   assigned-clock-rates = <0>, <12288000>;

   status = "okay";


  };
 };

 fragment@4 {
  target-path = "/";
  __overlay__ {
   sound-spdif {
    compatible = "fsl,imx-audio-spdif";
    model = "imx-spdif";
    spdif-controller = <&spdif>;
    spdif-in;
    spdif-out;
   };
  };
 };


 fragment@5 {

  target=<&spdif>;
  __overlay__ {
    pinctrl-names = "default";
   pinctrl-0 = <&pinctrl_spdif>;
   assigned-clocks = <&clks 69>,
       <&clks 107>;
   assigned-clock-parents = <&clks 50>;
   assigned-clock-rates = <0>, <49152000>;
   status = "okay";
  };

 };
};
